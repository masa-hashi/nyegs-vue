{
  "family": "golang",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "golang",
      "image": "golang",
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 81,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "executionRoleArn": "ecsTaskExecutionRole",
  "cpu": "256",
  "memory": "512"
}